{
  "extends": [
    "config:base"
  ],
  "enabledManagers": ["npm"],
  "packageRules": [
    {
      "matchPackagePatterns": ["kiltprotocol"],
      "groupName": "kiltprotocol"
    },
    {
      "matchPackagePatterns": ["parcel"],
      "groupName": "parcel"
    },
    {
      "matchPackagePatterns": ["@polkadot"],
      "groupName": "polkadot"
    }
  ]
}
